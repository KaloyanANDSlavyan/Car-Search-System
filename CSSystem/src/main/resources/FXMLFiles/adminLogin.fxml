<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="552.0" prefWidth="668.0" styleClass="loaderPane" stylesheets="@../style/design.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cssystem.Controllers.AdminLoginController">
   <children>
      <TextField fx:id="usernameField" layoutX="229.0" layoutY="193.0" prefHeight="26.0" prefWidth="210.0" promptText="Username:" styleClass="loginFields" />
      <PasswordField fx:id="passwordField" layoutX="229.0" layoutY="262.0" prefHeight="26.0" prefWidth="210.0" promptText="Password:" styleClass="loginFields" />
      <Button fx:id="loginButton" defaultButton="true" layoutX="235.0" layoutY="368.0" mnemonicParsing="false" onAction="#onClickLogin" prefHeight="26.0" prefWidth="197.0" styleClass="loginButton" text="LOGIN">
         <font>
            <Font name="Agency FB Bold" size="16.0" />
         </font></Button>
      <Label fx:id="loginFalseLabel" layoutX="235.0" layoutY="320.0" style="-fx-text-fill: red;" text="Username or password is not correct." visible="false" />
      <Label layoutX="211.0" layoutY="40.0" style="-fx-text-fill: #FFD700;" text="ADMINISTRATOR">
         <font>
            <Font name="Agency FB Bold" size="45.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
