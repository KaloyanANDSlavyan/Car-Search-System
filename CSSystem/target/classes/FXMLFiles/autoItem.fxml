<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="hbox" alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="762.0" spacing="10.0" styleClass="row" stylesheets="@../style/design.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cssystem.Controllers.AutoItemController">
   <children>
      <ImageView fx:id="imageView" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </ImageView>
      <Label fx:id="typeLabel" prefWidth="140.0" text="Type" textFill="GOLD">
         <font>
            <Font name="Agency FB" size="20.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="brandLabel" prefWidth="145.0" text="Brand" textFill="GOLD">
         <font>
            <Font name="Agency FB" size="20.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="modelLabel" prefWidth="150.0" text="Model" textFill="GOLD">
         <font>
            <Font name="Agency FB" size="20.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="priceLabel" prefWidth="100.0" text="Price" textFill="GOLD">
         <font>
            <Font name="Agency FB" size="20.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
         <children>
            <Button fx:id="viewButton" mnemonicParsing="false" onAction="#onClickView" prefHeight="40.0" prefWidth="40.0" styleClass="loginButton">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/1200px-Magnifying_glass_icon.svg.png" />
                     </image>
                  </ImageView>
               </graphic>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onClickDelete" prefHeight="40.0" prefWidth="40.0" styleClass="loginButton">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/download-removebg-preview.png" />
                     </image>
                  </ImageView>
               </graphic>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
   </children>
</HBox>
